{
  "name": "@studiorack/core",
  "version": "2.0.3",
  "description": "Core library containing shared functionality",
  "type": "module",
  "main": "./build/index.js",
  "exports": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "copy": "copyfiles -u 1 ./src/images/* ./build",
    "start": "node build/src/index.js",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build": "rimraf ./build && tsc && npm run copy",
    "dev": "tsx ./src/index.ts",
    "lint": "eslint ./**/*.ts",
    "test": "vitest run ./tests",
    "format": "prettier --write ./src && prettier --write ./tests",
    "test:watch": "vitest ./tests --hook-timeout=30000 --test-timeout=30000",
    "quicktype": "quicktype ./src/types/*.ts -o ./src/convert.ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiorack/studiorack-core.git"
  },
  "keywords": [
    "studiorack",
    "plugin",
    "library",
    "core",
    "package"
  ],
  "author": "kmturley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/studiorack/studiorack-core/issues"
  },
  "homepage": "https://github.com/studiorack/studiorack-core#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@vscode/sudo-prompt": "^9.3.1",
    "adm-zip": "^0.4.16",
    "glob": "^10.3.6",
    "node-fetch": "^2.6.1",
    "readline-sync": "^1.4.10",
    "semver": "^7.3.4",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/adm-zip": "^0.4.33",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.11",
    "@types/readline-sync": "^1.4.8",
    "@types/semver": "^7.5.8",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "prettier": "^3.2.5",
    "quicktype": "^23.0.168",
    "rimraf": "^5.0",
    "tsx": "^4.10.2",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.9.0",
    "vitest": "^1.6.0"
  }
}
